{
  "tech_stack": [
    "TypeScript",
    "Next.js 15",
    "React 18",
    "Tailwind CSS",
    "shadcn/ui",
    "Prisma ORM",
    "NextAuth.js",
    "MySQL/SQLite",
    "Zod",
    "React Hook Form",
